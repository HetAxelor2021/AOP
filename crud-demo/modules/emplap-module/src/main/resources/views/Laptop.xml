<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
 	
 	<grid name="laptop-grid" title="Laptops" model="com.hh.emplap.db.Laptop" >
 		<field name="lname" />
 		<field name="employeeSet" >
 			<grid title="Employees" >
 				<field name="name" />
 				<field name="email" />
 			</grid>
 		</field>
 	</grid>
 	<form name="laptop-form" title="Laptop" model="com.hh.emplap.db.Laptop">
 		<panel>
 			<field name="lname" />
 			<panel-related field="employeeSet"></panel-related>
 		</panel>
 	</form>
</object-views>

