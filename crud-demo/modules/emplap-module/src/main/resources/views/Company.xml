<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
  
	<grid name="company-grid" title="Companies" model="com.hh.emplap.db.Company" >
		<field name="name" />
		<field name="startDate" />
		<field name="logo" />
		<field name="clientSet" >
			<grid title="Clients" >
				<field name="name" />
			</grid>
		</field>
		
		<field name="employeeList" />
		
	</grid>
	<form name="company-form" title="Company" model="com.hh.emplap.db.Company" >
		<panel >
			<field name="name" />
			<field name="startDate" />
			<field name="logo" />
			<panel-related field="clientSet" />
			<panel-related field="employeeList" />
		</panel>
	</form>
</object-views>