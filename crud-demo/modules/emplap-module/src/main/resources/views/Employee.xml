<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.4.xsd">
	<grid name="employee-grid" title="Employees" model="com.hh.emplap.db.Employee" >
		<field name="id" />
		<field name="name" />
		<field name="email" />
		<field name="laptopSet" >
			<grid title="Laptops">
				<field name="id" />
				<field name="lname" />
				
			</grid>
		</field>
	</grid>
	<form model="com.hh.emplap.db.Employee" name="employee-form" title="Employee" >
		<panel>
			
			<field name="name" />
			<field name="email" />
			<panel-related field="laptopSet" />
				
			
		
		</panel>
		
	</form>
</object-views>
